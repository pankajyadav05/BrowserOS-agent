{
  "manifest_version": 3,
  "name": "Mitria",
  "version": "50.0.3.10",
  "description": "Mitria - AI-powered browser automation",
  "key": "",
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "scripting",
    "tabs",
    "tabGroups",
    "debugger",
    "sidePanel",
    "bookmarks",
    "history",
    "browserOS",
    "webNavigation",
    "downloads",
    "audioCapture"
  ],
  "update_url": "https://cdn.browseros.com/extensions/update-manifest.xml",
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; media-src 'self' blob: data: mediastream:; object-src 'self'; connect-src *"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "commands": {
    "toggle-panel": {
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "Command+E"
      },
      "description": "Toggle the Mitria side panel"
    }
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
